{
  "name": "backstop-template",
  "version": "1.0.0",
  "description": "A template for all projects using [BackstopJS](https://github.com/garris/BackstopJS)",
  "main": "index.js",
  "scripts": {
    "backstop": "backstop",
    "dev": "nodemon ./backstop/_utils/scenarios.js",
    "approve": "backstop approve --config='backstop/backstop.config.js'",
    "remote": "backstop remote --config='backstop/backstop.config.js'",
    "serve": "live-server ./project",
    "test": "backstop test --config='backstop/backstop.config.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "backstopjs": "^6.2.2"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "nodemon": "^3.0.1"
  }
}
